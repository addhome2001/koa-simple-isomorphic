{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "react/jsx-curly-spacing": [2, "always"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": [1, { "jsx": "never", "js": "never" }],
    "no-param-reassign": [1, { "props": false }],
    "react/prop-types": [1, { "ignore": ["params", "route"] }]
  }
}
