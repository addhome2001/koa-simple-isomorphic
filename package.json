{
  "name": "koa2-react-ssr",
  "version": "1.0.1",
  "description": "react server-side-render with koa2",
  "main": "dist/app",
  "scripts": {
    "start": "nodemon src --config",
    "test:watch": "nodemon ./node_modules/.bin/mocha --opts .mocharc ./test/*-spec.js",
    "test": "./node_modules/.bin/mocha --opts .mocharc ./test/*-spec.js",
    "lint": "./node_modules/.bin/eslint .",
    "validate": "npm run lint && npm test",
    "clear": "rimraf dist",
    "debug": "cross-env DEBUG=* npm start",
    "build": "npm run clear && cross-env NODE_ENV=production babel src -d dist --copy-files",
    "serve": "node dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "co": "4.6.0",
    "cross-env": "^3.2.4",
    "ejs": "2.4.2",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "nodemon": "1.10.0"
  },
  "dependencies": {
    "glamor": "^3.0.0-1",
    "glamor-react": "^3.0.0-1",
    "glamor-server": "^3.0.0-1",
    "koa": "2.0.0",
    "koa-bodyparser": "2.2.0",
    "koa-conditional-get": "1.0.3",
    "koa-convert": "1.2.0",
    "koa-csrf": "2.4.0",
    "koa-etag": "2.1.1",
    "koa-logger": "1.3.0",
    "koa-onerror": "^3.0.2",
    "koa-passport": "2.1.1",
    "koa-router": "7.0.1",
    "koa-session": "3.3.1",
    "koa-static-server": "^1.0.0",
    "koa-views": "5.0.1",
    "lru-cache": "^4.0.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "supertest": "2.0.0"
  }
}
